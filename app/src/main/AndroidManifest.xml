<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onstage">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnStage">
        <activity
            android:name="com.example.onstage.HomePageActivity" />
        <activity
            android:name="com.example.onstage.EditProfileActivity" />
        <activity
            android:theme="@style/Theme.OnStage"
            android:name="com.example.onstage.SettingsPageActivity"
            android:exported="true" />
        <activity
            android:theme="@style/Theme.OnStage"
            android:name="com.example.onstage.DetailActivity"
            android:exported="true" />
        <activity
            android:theme="@style/Theme.OnStage"
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>