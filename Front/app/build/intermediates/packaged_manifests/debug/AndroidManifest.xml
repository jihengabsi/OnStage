<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onstage"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="31" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.OnStage" >
        <activity android:name="com.example.onstage.HomePageActivity" />
        <activity android:name="com.example.onstage.EditProfileActivity" />
        <activity
            android:name="com.example.onstage.SettingsPageActivity"
            android:exported="true"
            android:theme="@style/Theme.OnStage" />
        <activity
            android:name="com.example.onstage.DetailActivity"
            android:exported="true"
            android:theme="@style/Theme.OnStage" />
        <activity
            android:name="com.example.onstage.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.OnStage" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>